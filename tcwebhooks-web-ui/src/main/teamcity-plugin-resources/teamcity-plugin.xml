<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>tcWebHooks</name>
    <display-name>Web Hooks plugin</display-name>
    <version>${version}</version>
    <description>Trigger webhook POST requests from build events. Adds tabs to the Project and Build pages.</description>
    <vendor>
      <name>Net Wolf</name>
      <url>http://netwolfuk.wordpress.com/teamcity-plugins/</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="false"/>
</teamcity-plugin>
